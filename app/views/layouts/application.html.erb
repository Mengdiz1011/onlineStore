<!DOCTYPE html>
<html>
  <head>
    <title>App278</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
 
  <body>
    <div id="banner">
      <h1>My Online Store<h1>
    </div>

  <div id="columns">
      <div id="side">
        <div id="cart">
          <%= render @cart%>
        </div>

    </div>
    
    <div id="main">
        <%= yield %>
    </div>
  </div>
  </body>
</html>
